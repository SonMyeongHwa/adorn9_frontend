{
	"env": {
		"browser": true,
		"commonjs": true,
		"es2021": true
	},
	"extends": "eslint:recommended",
	"parserOptions": {
		"ecmaVersion": "latest"
	},
	"rules": {
		"quotes": ["error", "single"],
		"semi": ["error", "always"],
		"prefer-const": "off",
		"no-unused-vars": "off"
	},
	"globals": {
		"navigate": "writable"
	}
}
